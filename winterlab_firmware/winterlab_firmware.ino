  int val = 0;
  int val1 = 0;
  int readfrom;
  //char which = Serial.read();
  if(which == 0) {
    readfrom = A0;
  }
  if(which == 1) {
    readfrom = A2;
  }
  for(int counter = 0; counter < numValues; counter++) {
    readValues0[counter] = analogRead(readfrom);

void getOhmMeter() {
  // Purpose: uses a voltage divider to measure the value of an unknown resistance.
  // Date added: Aug. 2017
  // Data modified: Jan 2019
  digitalWrite(meterPinOut, HIGH);
  int vreftot = 0;
  int vmtot = 0;
  int R2;
  for(int i = 0; i < 100; i ++) {
    vreftot = vreftot + analogRead(meterPinRef);
    vmtot = vmtot + analogRead(meterPinIn);    
  }
  digitalWrite(meterPinOut, LOW);
  float vrefavg = (float)vreftot/100.0;
  float vmavg = (float)vmtot/100.0;

  if(vmavg <= vrefavg*0.1 ){  
      R2 = -1; // Range error; range too small
    }
  else  if(vmavg >= vrefavg*0.9) {
      R2 = -2; // Range error: range too large
    } 
  else {
    R2 = rangeDMM*(vrefavg/vmavg - 1);
  }
  Serial.flush();
  Serial.println(R2);
}

void getCapMeter() {
  // Purpose: Determines the value of a capacitance attached at the measure point
  // Date added: Oct. 2018
  // Date modified: Feb 2019 added adaptive measurement delay to extend range

 
  byte measured[1000];
  byte ref[1000];
  float C;
  
  long timerend;
  byte success = 0;
  int i;
  long timerstart = micros();
  int range_delay=10;
  if(rangeDMM == 1000 || rangeDMM == 10000) {
    range_delay = 10;
  }
  else if(rangeDMM == 1000000) {
    range_delay = 20;
  }
  else {
    range_delay = 10;
  }
  digitalWrite(meterPinOut, HIGH);
  for(i=0; i<1000; i++) { 
    measured[i] = analogRead(meterPinIn);
    ref[i] = analogRead(meterPinRef);
    if(i>0) {
      if(measured[i-1] > measured[i]) {
   
        if(measured[i] <= 255.0*0.367) {
          timerend = micros() - timerstart;
          //Serial.println(timerend);
            if(i>20) {
              success = 1;
              }
            if(i<20) {
              success = -1; //range too low
              }
          }
        if(success != 0) {
          break;
        }
      }
    }
    delayMicroseconds(range_delay);   
  }
  digitalWrite(meterPinOut, LOW);

  if(success == 1) {
    C = (float)timerend/(float)rangeDMM;
  }
  else if(success == -1) {
    C = -1;
  }
  else {
    C = -2;
  }
  if(rangeDMM == 1000000) {
    Serial.println(C, 4);
  }
  else {
    Serial.println(C, 3);
  }
}


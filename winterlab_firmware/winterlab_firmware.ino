  Serial.println('0');
}


void prep() {
  // Purpose: populates the output tables for the DAC
  // Date added: Aug. 2017
  // Data modified: Jan. 2019: MRouble revamped strategy to allow arbitrary waveform generation 

  int val = 0;
  long shift;
  if(wave == 1) {
    for(int i = 0; i<bufferlen; i++) {
      pac = pac + m;
      if(pac >= pac_comp) {
        pac = pac - pac_comp;
      }
      shift =  (pac >> (m_extend));
      place = shift%((long)pow(2, table_res));
      
      val = amp*sinetable[place] + dcoffset - (amp*4095)*0.5;
      if(val > 4095) {
        val = 4095;
      }
      else if(val < 0) {
        val = 0;
      }  
      alttable[i] = val;
      //pac = pac + m;
      
    }
  }
  if(wave == 2) {
    for(int i = 0; i<bufferlen; i++) {

      pac = pac + m;
      if(pac >= pac_comp) {
        pac = pac - pac_comp;
      }
      shift =  (pac >> (m_extend));
      place = shift%((int)pow(2, table_res));
      val = amp*tritable[place] + dcoffset - (amp*4095)*0.5;
      if(val > 4095) {
        val = 4095;
      }
      else if(val < 0) {
        val = 0;
      }
      alttable[i] = val;
    }
  }
  if(wave == 3) {
    for(int i = 0; i<bufferlen; i++) {
      pac = pac + m;
      if(pac > pac_comp) {
        pac = pac - pac_comp;
      }
      shift =  (pac >> (m_extend));
      place = shift%((int)pow(2, table_res));
      val = amp*lintable[place] + dcoffset - (amp*4095)*0.5;
      if(val > 4095) {
        val = 4095;
      }
      else if(val < 0) {
        val = 0;
      }
      alttable[i] = val;
    }
  }
  if(wave == 4) {
    for(int i = 0; i<bufferlen; i++) {
      pac = pac + m;
      if(pac > pac_comp) {
        pac = pac - pac_comp;
      }
      shift =  (pac >> (m_extend));
      place = shift%((int)pow(2, table_res)); 
      val = amp*squaretable[place] + dcoffset - (amp*4095)*0.5;
      if(val > 4095) {
        val = 4095;
      }
      else if(val < 0) {
        val = 0;
      }
      alttable[i] = val;
    }
  }
  if(wave == 5) {
    for(int i = 0; i<bufferlen; i++) {
      pac = pac + m;
      shift =  (pac >> (m_extend));
      place = shift%((int)pow(2, table_res));

      if(pac > pac_comp) {
        pac = pac - pac_comp;
      }
      val = amp*noisetable[place] + dcoffset - (amp*4095)*0.5;
      if(val > 4095) {
        val = 4095;
      }
      else if(val < 0) {
        val = 0;
      }
      alttable[i] = val; 
    }
  }
  if(wave == 6) {
    for(int i = 0; i<bufferlen; i++) {
      val = dcoffset;
      if(val <= 4095 && val>0) {
        alttable[i] = val;   
      }
      else if(val <= 0) {
        alttable[i] = 0;
      }
      else {
        alttable[i] = 4095;
      }
    }
  }
  
  if(flag1 == true) {
    memcpy(table1, &alttable[0],sizeof(alttable));
    prep_flag = false;
    return;
  }
  else {
    memcpy(table2, &alttable[0], sizeof(alttable));
    prep_flag = false;
    return;
  }
}


void getTrigger() {
  // Purpose: reads oscilloscope trigger level (0 to 256)
  // Author: Maclean Rouble
  // Date added: Aug. 2017
  // Data modified: Oct. 2018
  Serial.readBytesUntil('\n', trigbuff, 4); // three digits indicate trigger level
  trigbuff[3] = '\0';
  trig = atoi(trigbuff);
  Serial.println('0');
}

void getTriggerChannel() {
  // Purpose: reads oscilloscope trigger channel (1 for ch1 or 0 for ch2)
  // Author: Maclean Rouble
  // Date added: Oct. 2018
  // Date modified: Oct. 2018
  Serial.readBytesUntil('\n', chbuff, 2);
  chbuff[1] = '\0';
  trigchannel = atoi(chbuff);
  Serial.println('0');
}

void twoChannelOsc() {
  // Purpose: use ADC to read values from both oscilloscope channels
  //          You can make this algorithm more efficient, but sending large arrays over serial is slow
  //            compared to sampling, so there's some headroom here, and atm I've gone for readability
  // Author: Maclean Rouble
  // Date added: Aug. 2017
  // Date modified: Oct. 2018
  int counter = 0;
  int maxval[2];
  int minval[2];
  int val[2];
  int prev[2];
  for(int i=0; i<=1;i++) {
    maxval[i] = 0;
    minval[i] = 255;
    val[i] = 0;
    prev[i] = 0;
  }
  
  int val1 = 0;
  int channel = 0;

  // do a full read through to find the max and min vals present on each channel
  for(channel = 0; channel <= 1; channel++) {
    for(counter = 0; counter < numValues; counter++) {
      result = adc->analogSynchronizedRead(readPin0, readPin1);
      if(channel == 0) {
        val[channel] = result.result_adc0;
      }
      else if(channel == 1) {
        val[channel] = result.result_adc1;
      }
      else {
        //digitalWrite(ledpin, !digitalRead(ledpin));
        break;
        channel = 0; 
        val[channel] = analogRead(readPin0);
      }
      readValues[channel][counter] = val[channel]; //type collision? analogRead returns byte or int?
    
      if(val[channel] > maxval[channel]) {
        maxval[channel] = val[channel];
      }
      if(val[channel] < minval[channel]) {
        minval[channel] = val[channel];
      } 
    }
  }
    // now actually record.
    // start recording on both channels when trig ch gets within range
    counter = 0;
    
    if(trig >= minval[trigchannel] && trig <= maxval[trigchannel]) { // if the trig level is inside the wave on trig ch, start checking
      digitalWrite(ledpin, HIGH);
      //Serial.println("In range");
      int checker = 0;
      while(checker == 0) {
        result = adc->analogSynchronizedRead(readPin0, readPin1);
        prev[0] = result.result_adc0;
        prev[1] = result.result_adc1;
        result = adc->analogSynchronizedRead(readPin0, readPin1);
        val[0] = result.result_adc0;
        val[1] = result.result_adc1;
        if(val[trigchannel] > trig && prev[trigchannel] <= trig) {
          checker = 1;
        }
        counter = counter + 1;
        if(counter > numValues) {
          checker = 1; // break if we don't find it to not get stuck in the loop
        }
      }

      for(counter = 0; counter < numValues; counter++) {
        result = adc->analogSynchronizedRead(readPin0, readPin1);
        readValues[0][counter] = result.result_adc0;
        readValues[1][counter] = result.result_adc1;
      }
      twoChannelSend();
    }
    else {
      twoChannelSend(); // if trig level is outside the wave, just send what we read during the first pass
    }
}


void twoChannelSend() {
  // Purpose: transmits recorded oscilloscope data to python.
  // Author: Maclean Rouble
  // Date added: Aug. 2017
  // Data modified: Oct. 2018
  
  Serial.flush();
  for (int i = 0; i < numValues; i++) {
    Serial.print(readValues[1][i]);
    Serial.print(".");
  }
  Serial.println("");
  delay(10);
  
  for (int i = 0; i < numValues; i++) {
    Serial.print(readValues[0][i]);
    Serial.print(".");
  }
  Serial.println("");
  digitalWrite(ledpin, LOW);
  delay(10);
}




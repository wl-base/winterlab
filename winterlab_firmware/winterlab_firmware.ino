void readFromSerial() {
  // Purpose: parses input from python GUI and directs board to take assigned action.
  // Date added: Aug. 2017
  // Data modified: Jan 2019
  
  startword = Serial.read();
  if (startword == 'F') {
    getFreq();
  }
  if (startword == 'A') {
    getAmp();
  }
  if (startword == 'S') {
    wave = 1; //sine
    Serial.println('0');
  }
  if (startword == 'R') {
    wave = 2; //tri
    Serial.println('0');
  }
  if (startword == 'U') {
    wave = 3; //saw
    Serial.println('0');
  }
  if (startword == 'Q') {
    wave = 4; //square
    Serial.println('0');
  }
  if (startword == 'W') {
    wave = 5; //noise
    Serial.println('0');
  }
  if (startword == 'B') {
    wave = 6; //off
    Serial.println('0');
  }
  if (startword == 'M') {
    getVoltMeter();
  }
  if (startword == 'O') {
    getOhmMeter();
  }
  if(startword == 'I') {
    getCapMeter();
  }
  if (startword == 'H') {
    getDMMRange();
  }
  if (startword == 'T') {
    getTrigger();
  }
  if (startword == 'C') {
    getTriggerChannel();
  }
  if (startword == 'D') {
    getOffset();
  }
  if (startword == 'Z') {
    twoChannelOsc();
  }
  if(startword == 'E') {
    measureRange(0);
  }
  if(startword == 'G') {
    measureRange(1);
  }
  if(startword == 'J') {
    getDC();
  }
  
  else {
    while (Serial.available()) {
      char t = Serial.read();
    }
  }
}


}


void getFreq() {
  // Purpose: Receives function generator frequency information from python. Updates tuning word and triggers output table regeneration.
  // Date added: Aug. 2017
  // Data modified: Nov 2018
  
  Serial.readBytesUntil('\n', freqbuff, 6);
  freqbuff[6] = '\0';
  int received = atoi(freqbuff);
  freq = (float)received;
  M = ((float)freq * (float)pow(2,table_res)) / (float)f_clk;
  m = M*pow(2, m_extend);
  Serial.println('0');
  delay(1);
}


void getAmp() { 
  // Purpose: reads and parses wave amplitude from python
  // Date added: Aug. 2017
  // Data modified: 
  
  Serial.readBytesUntil('\n', ampbuff, 5);
  ampbuff[5] = '\0';
  int received = atoi(ampbuff);
  amp = (float)received / 4095.0;
  ypos = 4096 - (10.0*amp + (5.0-5.0*amp))*409.6; 
  Serial.println('0');
  delay(1);
}

